        {
            "name": "postgresql",
            "targetType": "library",
            "subConfigurations": {
                "hunt-database": "postgresql"
            }
        },
        {
            "name": "sqlite",
            "targetType": "library",
            "subConfigurations": {
                "hunt-database": "sqlite"
            }
        }
