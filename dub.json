    "license": "Apache-2.0",
    "dependencies": {
        "hunt" : "~>1.0.0-beta.8",
        "hunt-database": "~>1.0.0",
        "hunt-sql": "~>1.0.0"
    },
    "configurations": [
        {
            "name": "default",
            "targetType": "library",    
            "subConfigurations": {
                "hunt-database": "mysql"
